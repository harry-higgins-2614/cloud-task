{
  "name": "stream-take-home-assignment",
  "version": "0.1.0",
  "bin": {
    "eda-esg-good-stream-listener": "bin/eda-esg-good-stream-listener.js"
  },
  "scripts": {
    "js:build": "webpack --entry ./src/index.js",
    "js:watch": "webpack watch --entry ./src/index.js --node-env development",
    "ts:build": "webpack --entry ./src/index.ts",
    "ts:watch": "webpack watch --entry ./src/index.ts --node-env development",
    "start:server": "node external-service",
    "invoke": "sam-beta-cdk local invoke stream-take-home-assignment/MyFunction --event event.json --env-vars env.json",
    "debug": "sam-beta-cdk local invoke stream-take-home-assignment/MyFunction --event event.json --env-vars env.json -d 5858",
    "test": "jest"
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "cdk/**/*.ts"
    ],
    "setupFiles": [
      "<rootDir>config.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": ".test\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.95.1",
    "@aws-cdk/aws-lambda": "1.95.1",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "aws-cdk": "1.95.1",
    "aws-sdk": "^2.834.0",
    "aws-sdk-mock": "^5.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unused-imports": "^1.0.1",
    "jest": "^26.6.3",
    "jest-ts-auto-mock": "^1.0.12",
    "prettier": "^2.2.1",
    "ts-auto-mock": "^3.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "webpack-cli": "^4.6.0",
    "webpack": "^5.36.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@aws-cdk/core": "1.95.1",
    "@types/aws-lambda": "^8.10.83",
    "jsonschema": "^1.4.0",
    "source-map-support": "^0.5.16"
  }
}
